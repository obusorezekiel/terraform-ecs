{
    "family":"my_task",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "my_task",
        "image": "my-sample-app",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000
          }
        ],
        "memory": 512,
        "cpu": 256
      }
    ]
}
  